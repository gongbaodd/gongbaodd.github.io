    "gatsby-plugin-csp": "1.1.3",
    "gatsby-plugin-feed": "2.3.26",
    "gatsby-plugin-github-ribbon": "1.0.1",
    "gatsby-plugin-google-analytics": "2.1.34",
    "gatsby-plugin-manifest": "2.2.39",
    "gatsby-plugin-offline": "3.0.32",
    "gatsby-plugin-page-progress": "2.0.16",
    "gatsby-plugin-react-helmet": "3.1.21",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sentry": "1.0.1",
    "gatsby-plugin-sharp": "2.4.3",
    "gatsby-plugin-sitemap": "^2.2.30",
    "gatsby-plugin-theme-ui": "0.3.0",
    "gatsby-plugin-tidy": "1.0.1",
    "gatsby-plugin-typescript": "2.2.0",
    "gatsby-plugin-typography": "2.3.25",
    "gatsby-remark-copy-linked-files": "2.1.36",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "3.1.49",
    "gatsby-remark-prismjs": "3.3.32",
    "gatsby-remark-responsive-iframe": "2.2.31",
    "gatsby-remark-smartypants": "2.1.20",
    "gatsby-source-filesystem": "2.1.46",
    "gatsby-transformer-remark": "2.6.53",
    "gatsby-transformer-sharp": "2.3.13",
    "moment": "2.24.0",
    "prismjs": "1.19.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-helmet": "5.2.1",
    "react-typography": "0.16.19",
    "source-map-support": "0.5.16",
    "theme-ui": "0.3.1",
    "ts-node": "8.6.2",
    "typeface-merriweather": "0.0.72",
    "typeface-montserrat": "0.0.75",
    "typeface-roboto": "0.0.75",
    "typography": "0.16.19",
    "typography-theme-doelger": "0.16.19",
    "with-utterances": "1.5.0",
    "xss": "1.0.6"
  },
  "devDependencies": {
    "@theme-ui/preset-deep": "0.3.0",
    "@types/cypress-cucumber-preprocessor": "1.14.1",
    "@types/dompurify": "2.0.1",
    "@types/node": "13.9.2",
    "@types/react": "16.9.23",
    "@types/react-dom": "16.9.5",
    "@typescript-eslint/parser": "2.21.0",
    "audit-ci": "2.5.1",
    "commitizen": "4.0.3",
    "cypress-cucumber-preprocessor": "2.0.1",
    "emoji-cz": "0.3.1",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jam3": "0.2.3",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.3",
    "eslint-plugin-react-hooks": "2.5.0",
    "gatsby-plugin-no-sourcemaps": "2.1.2",
    "gh-pages": "2.2.0",
    "husky": "4.2.3",
    "lint-staged": "10.0.8",
    "prettier": "1.19.1",
    "typescript": "3.8.3"
  },
  "homepage": "https://github.com/gatsbyjs/gatsby-starter-blog#readme",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gatsbyjs/gatsby-starter-blog.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn type && yarn audit:ci && lint-staged",
      "pre-push": "yarn clean && yarn build"
    }
  },
  "lint-staged": {
    "*.js(x)|*.json|*.ts(x)|*.markdown|*.md": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "emoji-cz"
    }
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  },
  "scripts": {
    "install:win32": "HUSKY_SKIP_INSTALL=1 yarn install",
    "dev": "yarn clean && gatsby develop --host=0.0.0.0",
    "build": "NODE_ENV=production gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md,ts,tsx}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint src --ext=ts,js,tsx --fix",
    "commit": "git-cz",
    "audit:ci": "audit-ci -m -a 1488 -a 1217",
    "type": "tsc --noEmit",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 0",
    "cy:test": "sh scripts/run_cypress.sh",
    "cp:cname": "sh ./scripts/cp_cname.sh",
    "deploy:coding": "sh ./scripts/deploy_to_coding.sh",
    "deploy": "gh-pages -d public -r https://$GH_TOKEN@github.com/gongbaodd/gongbaodd.github.io.git -b master"
  }
}
